a=int(input("first"))
b=int(input("second"))
ch=int(input("1 for add /n 2 for sub /n 3 for mul/n 4 for div /n enter the choice"))
if(ch==1):
    print("sum",a+b)
elif(ch==2):
    print("sub",a-b)
elif(ch==3):
    print("mul",a*b)
elif(ch==4):
    print("div",a/b)
else:
    print("enter valid statment")
